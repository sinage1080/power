    // GLightbox instance for image
    var imageLightbox = glightbox__WEBPACK_IMPORTED_MODULE_4___default()({
        selector: '.glightbox-image', // Target video elements
        openEffect: 'fade',
        closeEffect: 'fade',
    });

    // GLightbox instance for videos
    var videoLightbox = glightbox__WEBPACK_IMPORTED_MODULE_4___default()({
        selector: '.glightbox-video', // Target video elements
        touchNavigation: true,
        loop: false,
        zoomable: true,
        autoplayVideos: false,
        closeOnOutsideClick: true,
        openEffect: 'fade',
        draggable: false,
        closeEffect: 'fade',
        closeButton: true, // Enable close button for videos
        plyr: {
            config: plyr_options,
        },
    });

    videoLightbox.on('open', function () {
        $('.gslide-media').append($('.gclose'));
    });

    var modalLightbox = glightbox__WEBPACK_IMPORTED_MODULE_4___default()({
        selector: '.glightbox-modal', // Target modal elements
        touchNavigation: true,
        loop: false,
        draggable: false,
        zoomable: true,
        autoplayVideos: false, // No autoplay needed for modals
        closeOnOutsideClick: true,
        height: 'auto',
        openEffect: 'fade',
        closeEffect: 'fade',
        closeButton: false, // Disable close button for modals
        plyr: null, // No Plyr config needed for modals
    });

    $('.glightbox-modal').on('click', function (e) {
        e.preventDefault();

        // Get the href attribute of the clicked element
        var targetHref = $(this).attr('href');

        // Set the elements for GLightbox
        modalLightbox.setElements([{ href: targetHref }]);

        // Open the lightbox
        modalLightbox.open();
    });

    // Navigation Toggle
    $('.component-nav-toggle').on('click', function () {
        var $control = $(this).attr('aria-controls');

        if ($(this).attr('aria-expanded') === 'true') {
            $('#' + $control).attr('aria-hidden', 'true');

            if ($control === 'search-modal') {
                $('body').removeClass('search-open');
                $(this).attr('aria-controls', 'primary-nav');
                $('.component-search-toggle').attr('aria-expanded', 'false');
            } else {
                $('header').removeClass('active');
                $('body').removeClass('modal-open');
            }

            if (!$('header').hasClass('active')) {
                $(this).attr('aria-expanded', 'false');
            }
        } else {
            $(this).attr('aria-expanded', 'true');
            $('#' + $control).attr('aria-hidden', 'false');
            $('body').addClass('modal-open');
            $('header').addClass('active');
        }
    });

    $('.menu-item-has-children > a').on('click', function (e) {
        e.preventDefault(); // Prevent the default anchor click behavior
        var $submenu = $(this).siblings('.sub-menu');

        // Slide toggle the submenu
        $submenu.slideToggle();

        // Optionally, toggle an active class for styling purposes
        $(this)
            .parent()
            .attr('aria-expanded', function (index, attr) {
                return attr == 'false' ? 'true' : 'false';
            });
    });

    $('.component-search-toggle').on('click', function (e) {
        e.preventDefault();
        var $control = $(this).attr('aria-controls');

        $(this).closest('.col').find('.component-nav-toggle').attr('aria-controls', 'search-modal');
        $(this).closest('.col').find('.component-nav-toggle').attr('aria-expanded', 'true');

        if ($(this).attr('aria-expanded') === 'true') {
            $('#' + $control).attr('aria-hidden', 'true');
            $(this).attr('aria-expanded', 'false');
            $('body').removeClass('search-open');
        } else {
            $(this).attr('aria-expanded', 'true');
            $('#' + $control).attr('aria-hidden', 'false');
            $('body').addClass('search-open');
        }

        //$('body').toggleClass('search-open');
        $('#search-modal').removeClass('search-entered');
        $('.search-body').html('');
        $('.search-footer').html('');
    });

    $('#search-modal').on('submit', 'form', function (e) {
        e.preventDefault();
        var search_term = $('#search-term').val();
        if (search_term != '') {
            $('#search-modal').addClass('search-entered');
            ajax_search();
        } else {
            $('#search-modal').removeClass('search-entered');
            $('.search-body').html('');
            $('.search-footer').html('');
        }
    });

    $('.search-footer').on('click', 'a', function (e) {
        e.preventDefault();
        var href = $(this).attr('href');
        var paged = getQueryStringValueFromHref(href, 'pagination');
        ajax_search(paged);
    });

    function searchModalReInit() {
        var header = $('header');
        var search = $('#search-modal');
        search.on('scroll', function () {
            if (search.scrollTop() > 0) {
                header.addClass('hide');
            } else {
                header.removeClass('hide');
            }
        });
        $('.search-body .component-tabs').on('click', '.tab', function (e) {
            e.stopPropagation();
            var tabs = $(this).closest('.component-tabs');
            $('.component-button', tabs).removeClass('component-button-dark-red component-button-active');
            $(this).find('.component-button').addClass('component-button-dark-red component-button-active');
            ajax_search();
        });
        $('#search-modal .component-dropdown').initializeDropdown({
            onOptionSelect: function (text, value) {
                var tabs = $(this).closest('.component-tabs');
                $('.tab[data-filter="' + value + '"]', tabs).trigger('click');
            },
        });
        $('#search-modal').scrollTop(0);
    }

    function getQueryStringValueFromHref(href, key) {
        var urlParams = new URLSearchParams(href.split('?')[1]);
        return urlParams.get(key);
    }

    function ajax_search(paged) {
        if ( paged === void 0 ) paged = 1;

        var search_term = $('#search-term').val();
        var filter = $('.component-button-active', $('.search-body .component-tabs')).parent().attr('data-filter');

        var data = {
            action: 'ajax_search',
            nonce: ajax.nonce,
            search_term: search_term,
            filter: filter,
            paged: paged,
        };

        $('#search-modal .search-body').html('<div class="loader"></div>');
        $('#search-modal .search-footer').html('');

        $.ajax({
            url: ajax.url,
            type: 'POST',
            cache: false,
            headers: { 'Cache-Control': 'no-cache, no-store, must-revalidate' },
            data: data,
            success: function (response) {
                $('#search-modal .search-body').html(response.data.results);
                $('#search-modal .search-footer').html(response.data.pagination);
                searchModalReInit();
            },
        });
    }

    if ($('.module-map').length > 0) {
        var mapModalLightbox = glightbox__WEBPACK_IMPORTED_MODULE_4___default()({
            skin: 'map',
            touchNavigation: true,
            loop: false,
            draggable: false,
            zoomable: true,
            autoplayVideos: false, // No autoplay needed for modals
            closeOnOutsideClick: false,
            keyboardNavigation: false,
            width: '918px',
            height: '608px',
            openEffect: 'fade',
            closeEffect: 'fade',
            closeButton: false, // Disable close button for modals
            plyr: null, // No Plyr config needed for modals
        });

        mapModalLightbox.on('open', function () {
            $('html,body').removeClass('glightbox-open');
            $('body').addClass('glightbox-open-map');
        });

        mapModalLightbox.on('close', function () {
            $('body').removeClass('glightbox-open-map');
        });

        $('.module-map').on('mouseenter', '.state-container', function () {
            var state_container = $(this);
            var state = $(this, state_container);
            var label = state.attr('data-label');
            var tooltip = $('.tooltip');
            var rect = state[0].getBoundingClientRect();
            var topCenterX = rect.left + rect.width / 2;
            var topCenterY = rect.top;
            tooltip.text(label);
            tooltip.css({
                left: topCenterX - tooltip.outerWidth() / 2 + 'px',
                top: topCenterY - tooltip.outerHeight() + 'px',
                opacity: 1,
            });
        });

        $('.module-map').on('mouseleave', '.state-container', function () {
            var tooltip = $('.tooltip');
            tooltip.css({
                opacity: 0,
            });
        });

        function setQueryParam(key, value) {
            // Get the current URL
            var url = new URL(window.location.href);

            // Update the query parameter
            url.searchParams.set(key, value);

            // Push the new URL to the browser's history without reloading the page
            history.pushState(null, '', url);
        }

        function getQueryParam(param) {
            var urlParams = new URLSearchParams(window.location.search);
            return urlParams.get(param);
        }

        function removeQueryParam(param) {
            // Get the current URL
            var url = new URL(window.location);

            // Check if the specified parameter exists
            if (url.searchParams.has(param)) {
                // Remove the parameter
                url.searchParams.delete(param);

                // Update the URL without reloading the page
                history.replaceState(null, '', url.toString());
            }
        }

        if ($('.component-dropdown[data-type="states"]').length > 0) {
            $('.component-dropdown[data-type="states"]').initializeDropdown({
                onOptionSelect: function (text, value) {
                    // If the new selection is the same as the previous one, do nothing
                    if (lastSelectedState === text) {
                        return;
                    }

                    // Update the last selected option
                    lastSelectedState = text;

                    var container = $(this).closest('.module-map');

                    if ($('.module-map.advanced-map').length > 0) {
                        if (text == 'National Overview') {
                            $('#map-modal .gtrigger-close').trigger('click');
                        } else {
                            $('.module-map.advanced-map .state-container[data-label="' + text + '"]').trigger('click');
                        }
                    } else {
                        if (text == 'National Overview') {
                            $('.state-container', container).removeClass('selected');
                            ajax_simple_map_data(text, container);
                        } else {
                            $('.module-map.simple-map .state-container[data-label="' + text + '"]').trigger('click');
                        }
                    }
                },
            });
        }

        if ($('.component-dropdown[data-type="map"]').length > 0) {
            $('.component-dropdown[data-type="map"]').initializeDropdown({
                onOptionSelect: function (text, value) {
                    var tabs = $(this).closest('.component-tabs');
                    $('.tab[data-filter="' + value + '"]', tabs).trigger('click');
                },
            });
        }

        if ($('.module-map.advanced-map').length > 0) {
            var state = getQueryParam('state');
            if (state) {
                ajax_advanced_map_data(state);
                set_active_option($('.component-dropdown[data-type="states"]'), state);

                mapModalLightbox.setElements([{ href: '#map-modal' }]);
                mapModalLightbox.open();

                // Close modal on close button click
                $('#map-modal .gtrigger-close').on('click', function () {
                    closeMapModal();
                });

                // Detect clicks outside of the #map-modal to trigger close button
                handleOutsideModalEvent();
                $(window).on('resize', handleOutsideModalEvent);
            }
        }

        $('.module-map.advanced-map').on('click', '.state-container', function () {
            var state = $(this);
            var label = state.attr('data-label');
            var slug = state.attr('data-slug');
            var container = state.closest('.module-map');
            var isSelected = state.hasClass('selected');

            // Reset any previous selections
            container.removeClass('blur-selected');
            $('.state-container', container).removeClass('selected');

            if (!isSelected) {
                container.addClass('blur-selected');
                state.addClass('selected');
                $('header').addClass('hide-on-modal');

                // Open the modal
                var activeFilters = []; // Reset the array
                // Loop through each .tab to find active buttons
                $('.tab', container).each(function () {
                    var button = $(this).find('.component-button-active');
                    if (button.length > 0) {
                        var filterValue = $(this).data('filter'); // Get the data-filter value
                        activeFilters.push(filterValue); // Add to the array
                    }
                });
                var activeFilter = activeFilters[activeFilters.length - 1];
                ajax_advanced_map_data(label, activeFilter);
                mapModalLightbox.setElements([{ href: '#map-modal' }]);
                mapModalLightbox.open();

                setQueryParam('state', slug);

                set_active_option($('.component-dropdown[data-type="states"]', container), slug);

                // Close modal on close button click
                $('#map-modal .gtrigger-close').on('click', function () {
                    closeMapModal();
                });

                // Detect clicks outside of the #map-modal to trigger close button
                handleOutsideModalEvent();
                $(window).on('resize', handleOutsideModalEvent);
            }
        });

        function handleOutsideModalEvent() {
            setTimeout(function () {
                // Define the event handler function
                var outsideModalHandler = function (e) {
                    if (!$(e.target).closest('#map-modal').length && !$(e.target).closest('.state-container').length) {
                        $('#map-modal .gtrigger-close').trigger('click');
                    }
                };

                // Check the screen width
                if (window.innerWidth > 1024) {
                    // Unbind first to prevent duplicate bindings
                    $(document).off('click.outsideModal').on('click.outsideModal', outsideModalHandler);
                } else {
                    // Unbind the event for smaller screens
                    $(document).off('click.outsideModal');
                }
            }, 500); // Add a delay of 500ms
        }

        // Function to close the modal and reset classes
        function closeMapModal() {
            removeQueryParam('state');

            $('header').removeClass('hide-on-modal');
            $('.module-map.advanced-map').removeClass('blur-selected');
            $('.module-map.advanced-map .state-container').removeClass('selected');
            $('#map-modal .modal-content').html('');

            // Unbind the click outside listener when closing the modal
            $(document).off('click.outsideModal');
            set_active_option($('.component-dropdown[data-type="states"]'), 'national-overview');
        }

        /*
        $('.module-map.advanced-map .component-dropdown').on('click', '.option', function (e) {
            const tabs = $(this).closest('.component-tabs');
            const option = $(this).data('value');
            $('.tab[data-filter="' + option + '"]', tabs).trigger('click');
        });
        */

        $('.module-map.advanced-map').on('click', '.tab', function (e) {
            var tabs = $(this).closest('.component-tabs');

            // Close all dropdowns first, except the one being clicked
            $('.tab.has-dropdown', tabs).not(this).removeClass('open-dropdown');

            if ($(this).hasClass('has-dropdown')) {
                // If the clicked tab has a dropdown, toggle the dropdown without affecting active classes
                $(this).toggleClass('open-dropdown');
            } else {
                // Remove active classes from all component buttons
                $('.component-button', tabs).removeClass('component-button-dark-red component-button-active');

                // Add active classes only to the clicked component button
                $(this).find('.component-button').addClass('component-button-dark-red component-button-active');

                // If the clicked tab is inside a dropdown, close the parent dropdown
                var dropdownParent = $(this).closest('.tab.has-dropdown');
                if (dropdownParent.length) {
                    dropdownParent.removeClass('open-dropdown');
                    // Add active classes to the parent component button as well
                    dropdownParent
                        .find('> .component-button')
                        .addClass('component-button-dark-red component-button-active');
                }

                // Only apply the filter if the clicked tab does not have the 'has-dropdown' class
                var filterCategories = $(this).data('filter').split(','); // Get the filter categories from the clicked tab as an array

                set_active_option($('.component-dropdown[data-type="map"]', tabs), filterCategories);

                // Loop through each state and add or remove 'active' class based on data-category
                $('.state-container').each(function () {
                    var stateCategories = $(this).data('category').split(','); // Get categories for each state as an array

                    // Check if every category in filterCategories is present in stateCategories
                    var matchesAll = filterCategories.every(function (category) { return stateCategories.includes(category.trim()); });

                    if (matchesAll) {
                        $(this).addClass('active'); // Add active if it matches all filter categories
                    } else {
                        $(this).removeClass('active'); // Remove active if it doesn't match all
                    }
                });

                var activeCount = $('.map .state-container.active').length;
                var inactiveCount = $('.map .state-container').length - activeCount;
                $('.legends li.active span').html('(' + activeCount + ')');
                $('.legends li.inactive span').html('(' + inactiveCount + ')');
            }

            e.stopPropagation(); // Prevent click from propagating to the document
        });

        // Close dropdown when clicking outside of it
        $(document).on('click', function (e) {
            if (!$(e.target).closest('.component-tabs').length) {
                $('.tab.has-dropdown').removeClass('open-dropdown');
            }
        });

        $('.module-map.simple-map').on('click', '.state-container', function () {
            var state = $(this);
            var label = state.attr('data-label');
            var slug = state.attr('data-slug');
            var container = state.closest('.module-map');
            var isSelected = state.hasClass('selected');
            $('.state-container', container).removeClass('selected');
            if (!isSelected) {
                state.addClass('selected');
                lastSelectedState = label;
                set_active_option($('.component-dropdown[data-type="states"]', container), slug);
                ajax_simple_map_data(label, container);
            } else {
                set_active_option($('.component-dropdown[data-type="states"]', container), 'national-overview');
                ajax_simple_map_data(null, container);
            }
        });

        function ajax_simple_map_data(state, container) {
            var container_id = container.attr('id');
            var simple_map_data = JSON.parse($('.data', container).attr('data-map'));

            var data = {
                action: 'ajax_simple_map_data',
                nonce: ajax.nonce,
                simple_map_data: simple_map_data,
            };

            if (state) {
                data.state = state;
            }

            $('.data', container).html('<div class="loader"></div>');

            $.ajax({
                url: ajax.url,
                type: 'POST',
                cache: false,
                headers: { 'Cache-Control': 'no-cache, no-store, must-revalidate' },
                data: data,
                success: function (response) {
                    $('.data', container).html(response.data.sidebar);
                    var targetSwiperContainer = $('#' + container_id + '-callout-carousel');
                    targetSwiperContainer.html(response.data.slider);

                    // Reinitialize only the specific Swiper instance
                    initializeSwiper(targetSwiperContainer);
                },
            });
        }
    }

    function ajax_advanced_map_data(state, activeFilter) {
        if ( activeFilter === void 0 ) activeFilter = false;

        var data = {
            action: 'ajax_advanced_map_data',
            nonce: ajax.nonce,
        };

        if (state) {
            data.state = state;
        }

        $('#map-modal .modal-content').html('<div class="loader"></div>');

        $.ajax({
            url: ajax.url,
            type: 'POST',
            data: data,
            cache: false,
            headers: { 'Cache-Control': 'no-cache, no-store, must-revalidate' },
            success: function (response) {
                $('#map-modal .modal-content').html(response.data.modal);

                $('#map-modal .component-dropdown').initializeDropdown({
                    onOptionSelect: function (text, value) {
                        $('#map-modal .modal-content .tab[data-filter="' + value + '"]').trigger('click');
                    },
                });

                $('#map-modal .modal-content').on('click', '.tab', function (e) {
                    var tabs = $(this).closest('.component-tabs');

                    // Close all dropdowns first, except the one being clicked
                    $('.tab.has-dropdown', tabs).not(this).removeClass('open-dropdown');

                    if ($(this).hasClass('has-dropdown')) {
                        // If the clicked tab has a dropdown, toggle the dropdown without affecting active classes
                        $(this).toggleClass('open-dropdown');
                    } else {
                        // Remove active classes from all component buttons
                        $('.component-button', tabs).removeClass('component-button-dark-red component-button-active');

                        // Add active classes only to the clicked component button
                        $(this).find('.component-button').addClass('component-button-dark-red component-button-active');

                        // If the clicked tab is inside a dropdown, close the parent dropdown
                        var dropdownParent = $(this).closest('.tab.has-dropdown');
                        if (dropdownParent.length) {
                            dropdownParent.removeClass('open-dropdown');
                            // Add active classes to the parent component button as well
                            dropdownParent
                                .find('> .component-button')
                                .addClass('component-button-dark-red component-button-active');
                        }

                        // Only apply the filter if the clicked tab does not have the 'has-dropdown' class
                        var filterCategory = $(this).attr('data-filter').trim(); // Get the filter category from the clicked tab
                        set_active_option($('.component-modal .component-dropdown'), filterCategory);

                        // Loop through each state and add or remove 'active' class based on data-category
                        $('#map-modal .modal-content .data-block').each(function () {
                            var dataBlock = $(this).attr('data-category').trim(); // Get category for each state

                            // Check if the full string values match exactly
                            if (dataBlock === filterCategory) {
                                $(this).addClass('active'); // Add active if it matches the full filter category
                            } else {
                                $(this).removeClass('active'); // Remove active if it doesn't match
                            }
                        });
                    }
                    e.stopPropagation(); // Prevent click from propagating to the document
                });

                $('#map-modal .modal-content .tab[data-filter="' + activeFilter + '"]').trigger('click');
            },
        });
    }

    if ($('.swiper').length > 0) {
        initializeSwiper();
    }

    function initializeSwiper(swiperContainer) {
        if ( swiperContainer === void 0 ) swiperContainer = null;

        // If `swiperContainer` is provided, initialize only that container
        var swiperElements = swiperContainer ? $(swiperContainer).find('.swiper') : $('.swiper');

        swiperElements.each(function () {
            var swiperElement = $(this);

            // Destroy existing Swiper instance if already initialized
            if (swiperElement[0].swiper) {
                swiperElement[0].swiper.destroy(true, true);
            }

            // Get Swiper settings
            var swiperSettings = swiperElement.data('swiper');
            var swiperExtraSettings = swiperElement.data('swiper-extra');

            // Swiper configuration
            var swiperConfig = Object.assign({}, swiperSettings,
                {loop: false,
                watchSlidesProgress: true,
                pagination: {
                    el: swiperElement.closest('.module').find('.swiper-pagination')[0],
                    clickable: true,
                },
                navigation: {
                    nextEl: swiperElement.closest('.module').find('.swiper-button-next')[0],
                    prevEl: swiperElement.closest('.module').find('.swiper-button-prev')[0],
                }});

            // Add custom pagination rendering if specified
            if (swiperExtraSettings && swiperExtraSettings.numberedPagination) {
                swiperConfig.pagination.renderBullet = function (index, className) {
                    return ("<span class=\"" + className + "\">" + (index + 1) + "</span>");
                };
            }

            // Initialize Swiper
            new swiper_bundle__WEBPACK_IMPORTED_MODULE_3__["default"](swiperElement[0], swiperConfig);
        });
    }

    // Header
    var $navToggle = $('.nav-toggle');
    var $header = $('header'); // Cache the header selector
    var lastScrollTop = 0;

    function throttle(func, limit) {
        var lastFunc;
        var lastRan;
        return function () {
            var context = this;
            var args = arguments;
            if (!lastRan) {
                func.apply(context, args);
                lastRan = Date.now();
            } else {
                clearTimeout(lastFunc);
                lastFunc = setTimeout(
                    function () {
                        if (Date.now() - lastRan >= limit) {
                            func.apply(context, args);
                            lastRan = Date.now();
                        }
                    },
                    limit - (Date.now() - lastRan)
                );
            }
        };
    }

    function scrollHide(firstload) {
        var currentScrollTop = $(window).scrollTop();

        if (currentScrollTop > lastScrollTop && currentScrollTop > 0 && !firstload) {
            $header.addClass('hide');
        } else {
            $header.removeClass('hide');
        }

        if (currentScrollTop > 0) {
            $header.addClass('scrolling');
        } else {
            $header.removeClass('scrolling');
        }

        lastScrollTop = currentScrollTop;
    }

    var throttledScrollHide = throttle(function () {
        scrollHide(false);
    }, 100);

    $(window).on('scroll', function () {
        requestAnimationFrame(throttledScrollHide);
    });

    scrollHide(true);

    // Animated Text

    if ($('.animated-lines').length > 0) {
        $('.animated-lines').each(function () {
            var $this = $(this);
            var lines = $this.children();
            var current_index = 0;
            var animation_duration = 0.5;
            var delay_between_animations = 1.5;
            var max_height = 0;

            // Calculate the maximum height of child elements
            lines.each(function () {
                var height = $(this).height();
                if (height > max_height) {
                    max_height = height;
                }
            });

            // Set the height of the animate_line
            $this.height(max_height);

            show_next_line();

            function show_next_line() {
                gsap__WEBPACK_IMPORTED_MODULE_0__.gsap.to(lines[current_index], {
                    opacity: 1,
                    y: 0,
                    duration: animation_duration,
                    onComplete: function () {
                        gsap__WEBPACK_IMPORTED_MODULE_0__.gsap.to(lines[current_index], {
                            opacity: 0,
                            y: -20,
                            duration: animation_duration,
                            delay: delay_between_animations,
                            onComplete: function () {
                                // Reset properties for the next cycle
                                gsap__WEBPACK_IMPORTED_MODULE_0__.gsap.set(lines[current_index], {
                                    opacity: 0,
                                    y: 20,
                                });
                                current_index = (current_index + 1) % lines.length;
                                show_next_line(); // Start next animation
                            },
                        });
                    },
                });
            }
        });
    }

    // Removes all active tabs
    function remove_active_tabs($tabs) {
        $tabs.find('.component-button').removeClass('component-button-dark-red component-button-active');
    }

    // Sets clicked tab to active
    function set_active_tab($this) {
        $this.find('.component-button').addClass('component-button-dark-red component-button-active');
    }

    if ($('.component-tabs.tabs').length > 0) {
        $('.component-tabs.tabs').on('click', '.tab', function () {
            var $tabs = $(this).closest('.component-tabs');
            var $module = $(this).closest('.module');
            var dataAttribute = $(this).data('filter');
            var $targetTab = $module.find('div[data-tab=' + dataAttribute + ']');

            // Proceed only if the target tab exists
            if ($targetTab.length) {
                remove_active_tabs($tabs);
                set_active_tab($(this));

                $module.find('div[data-tab]').removeClass('active');
                $targetTab.addClass('active');
            }
        });
    }

    if ($('.component-tabs.staff-tabs').length > 0) {
        $('.component-tabs.staff-tabs').on('click', '.tab', function () {
            var $tabs = $(this).closest('.component-tabs');
            remove_active_tabs($tabs);
            set_active_tab($(this));

            var dataAttribute = $(this).data('filter');
            var $module = $(this).closest('.module');

            $module.find('div[data-staff-type]').addClass('inactive');
            //$module.find('div[data-staff-type=' + dataAttribute + ']');
            var $targetTab = $module.find('div[data-staff-type]').filter(function () {
                var staffTypes = $(this).data('staff-type').split(',');
                return staffTypes.includes(dataAttribute);
            });
            $targetTab.removeClass('inactive');

            if (dataAttribute === 'all') {
                $module.find('div[data-staff-type]').removeClass('inactive');
            }
            set_active_option($tabs.find('.component-dropdown'), dataAttribute);
        });
    }

    function set_active_option($dropdown, value, settings) {
        var $dropdownOptionsParent = $dropdown.find('.dropdown');
        var $dropdownTrigger = $dropdown.find('.dropdown-trigger');
        var $dropdownCurrentSelected = $dropdown.find('.dropdown-trigger .selected');
        var $targetOption = $dropdownOptionsParent.find('.options ul li[data-value="' + value + '"]');

        if ($targetOption.length) {
            var selectedText = $targetOption.text().trim();
            var selectedValue = $targetOption.attr('data-value');

            // Update the selected text in the dropdown
            $dropdownCurrentSelected.text(selectedText);

            // Update the active class
            $dropdown.find('.options ul li').removeClass('selected');
            $targetOption.addClass('selected');

            // Trigger any optional callback
            if (settings) {
                if (settings.onOptionSelect) {
                    settings.onOptionSelect.call($dropdown[0], selectedText, selectedValue);
                }
            }

            // Hide the dropdown options
            $dropdownOptionsParent.find('.options').slideUp();
            $dropdownTrigger.attr('aria-expanded', 'false');
            $dropdownOptionsParent.attr('aria-hidden', 'true');
        }
    }

    function updateUrlParameter(key, value) {
        var url = new URL(window.location.href);
        url.searchParams.delete('pagination');

        if (value === 'all') {
            // Remove the parameter if the value is "all"
            url.searchParams.delete(key);
        } else {
            // Set or update the parameter otherwise
            url.searchParams.set(key, value);
        }

        // Reload the page with the new URL
        window.location.href = url.toString();
    }

    if ($('.component-dropdown[data-type="default"]').length > 0) {
        $('.component-dropdown[data-type="default"]').initializeDropdown({
            onOptionSelect: function (text, value) {
                // const dataName = $(this).closest('.component-dropdown').data('name');
                // updateUrlParameter(dataName, value);
                // ScrollTrigger.refresh();
                var $tabs = $(this).closest('.component-tabs');
                var $tab = $tabs.find('.tab[data-filter="' + value + '"]');
                remove_active_tabs($tabs);
                set_active_tab($tab);

                var $module = $(this).closest('.module');

                $module.find('div[data-staff-type]').addClass('inactive');
                $module.find('div[data-staff-type=' + value + ']').removeClass('inactive');

                if (value === 'all') {
                    $module.find('div[data-staff-type]').removeClass('inactive');
                }
            },
        });
    }

    if ($('.component-dropdown[data-type="page"]').length > 0) {
        $('.component-dropdown[data-type="page"]').initializeDropdown({
            onOptionSelect: function (text, value) {
                var dataName = $(this).closest('.component-dropdown').data('name');
                updateUrlParameter(dataName, value);
                gsap_ScrollTrigger__WEBPACK_IMPORTED_MODULE_1__.ScrollTrigger.refresh();
            },
        });
    }

    if ($('.component-dropdown[data-type="hash"]').length > 0) {
        $('.component-dropdown[data-type="hash"]').initializeDropdown({
            onOptionSelect: function (text, value) {
                window.location.hash = value;
            },
        });
    }

    $(document).on('input', 'textarea', function () {
        $(this).css('height', 'auto'); // Reset height
        $(this).css('height', this.scrollHeight + 'px'); // Adjust to scroll height
    });

    if ($('.component-accordion').length > 0) {
        $('.component-accordion .accordion-trigger').on('click', function () {
            $(this).parent().toggleClass('active');
            $('.accordion-panel', $(this).parent()).attr('aria-hidden', function (index, attr) {
                return attr == 'false' ? 'true' : 'false';
            });

            $(this).attr('aria-expanded', function (index, attr) {
                return attr == 'false' ? 'true' : 'false';
            });
            $('.accordion-panel', $(this).parent()).slideToggle();
            gsap_ScrollTrigger__WEBPACK_IMPORTED_MODULE_1__.ScrollTrigger.refresh();
        });
    }

    if (window.innerWidth > 1024) {
        // Check if the browser width is larger than 1024px
        if ($('.module-vertical-carousel').length > 0) {
            function adjustSlidesHeight() {
                var maxHeight = 0;

                // Find the tallest slide
                $('.module-vertical-carousel .slide').each(function () {
                    var slideHeight = $(this).outerHeight(true); // Include margins
                    if (slideHeight > maxHeight) {
                        maxHeight = slideHeight;
                    }
                });

                // Set .slides height to the tallest .slide
                $('.module-vertical-carousel .slides').css('height', maxHeight + 'px');
            }

            // Adjust height on page load and window resize
            adjustSlidesHeight();

            var timeline = gsap__WEBPACK_IMPORTED_MODULE_0__.gsap.timeline({
                scrollTrigger: {
                    trigger: '.module-vertical-carousel',
                    start: 'top 15%',
                    end: '+=' + 2000,
                    scrub: 1,
                    pin: $('.site-main'),
                    onUpdate: function (self) {
                        var progress = self.progress;
                        var totalSlides = $('.module-vertical-carousel .slide').length;
                        var index = Math.min(Math.floor(progress * totalSlides), totalSlides - 1);
                        var activeSlide = $('.module-vertical-carousel .slide').eq(index);
                        var activePagination = $('.module-vertical-carousel .slides-pagination-bullet').eq(index);

                        // Remove 'active' class from all slides and add to the current slide
                        $('.module-vertical-carousel .slide').removeClass('active');
                        activeSlide.addClass('active');

                        $('.module-vertical-carousel .slides-pagination-bullet').removeClass('active');
                        activePagination.addClass('active');

                        // Incrementally move the slides container up by 32px for each active slide
                        var offset = -index * 32; // Calculate offset based on active slide index
                        var margin_top = index * 32; // Calculate offset based on active slide index
                        $('.module-vertical-carousel .slides').css('transform', ("translateY(" + offset + "px)"));
                        $('.module-vertical-carousel .slides-pagination').css('margin-top', (margin_top + "px"));
                    },
                },
            });

            $('.module-vertical-carousel .slides-pagination-bullet').on('click', function () {
                var index = $(this).index(); // Get the index of the clicked bullet
                var totalSlides = $('.module-vertical-carousel .slide').length;

                // Calculate the progress value corresponding to the slide
                var targetProgress = index / (totalSlides - 1);

                // Access the ScrollTrigger instance directly
                var scrollTrigger = timeline.scrollTrigger;

                // Calculate the exact scroll target based on progress
                var startOffset = scrollTrigger.start; // Starting scroll position
                var endOffset = scrollTrigger.end; // Ending scroll position
                var targetScrollY = startOffset + targetProgress * (endOffset - startOffset);

                // Scroll to the calculated position
                gsap__WEBPACK_IMPORTED_MODULE_0__.gsap.to(window, {
                    scrollTo: {
                        y: targetScrollY,
                        autoKill: false, // Prevent interruptions by user scrolling
                    },
                    duration: 0.5,
                });
            });
        }
    }

    // Gravity Forms label effect
    if ($('.gform_wrapper').length > 0) {
        attachFocusBlurHandlers();

        function attachFocusBlurHandlers() {
            $('.gform_wrapper input, .gform_wrapper textarea').on('focus', function () {
                $(this).closest('.gfield').addClass('focused');
            });

            $('.gform_wrapper input, .gform_wrapper textarea').on('blur', function () {
                if ($(this).val() === '') {
                    $(this).closest('.gfield').removeClass('focused');
                }
            });

            // Check and add 'focused' class to inputs or textareas with values
            $('.gform_wrapper input, .gform_wrapper textarea').each(function () {
                if ($(this).val() !== '') {
                    $(this).closest('.gfield').addClass('focused');
                }
            });
        }

        $(document).on('gform_post_render', function (event, form_id) {
            attachFocusBlurHandlers();
        });
    }
});

})();

