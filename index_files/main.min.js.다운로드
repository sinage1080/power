        },
        vimeo: {
            byline: false,
            portrait: false,
            title: false,
            speed: true,
            transparent: false,
        },
    };

    var players = plyr__WEBPACK_IMPORTED_MODULE_5___default().setup('.js-player', {
        ratio: '16:9',
        controls: ['play-large', 'play', 'progress', 'current-time', 'mute', 'volume', 'fullscreen'],
    });

    /*
