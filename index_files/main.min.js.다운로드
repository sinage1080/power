jQuery(document).ready(function ($) {
    // Initialize the dropdown with optional callbacks
    $.fn.initializeDropdown = function (options) {
        var settings = $.extend(
            {
                onOptionSelect: null,
            },
            options
        );

        return this.each(function () {
            var $dropdown = $(this);
            var $dropdownOptionsParent = $dropdown.find('.dropdown');
            var $dropdownTrigger = $dropdown.find('.dropdown-trigger');

            // Toggle dropdown open/close with slide effect
            $dropdownTrigger.on('click', function () {
                $dropdownOptionsParent.find('.options').slideToggle();
                $dropdownOptionsParent.attr('aria-hidden', function (index, attr) {
                    return attr == 'false' ? 'true' : 'false';
                });
                $dropdownTrigger.attr('aria-expanded', function (index, attr) {
                    return attr == 'false' ? 'true' : 'false';
                });
            });

            // Update selected value and hide options when clicking on an option
            $dropdownOptionsParent.find('.options ul').on('click', 'li', function () {
                set_active_option($dropdown, $(this).attr('data-value'), settings);
            });

            // Close dropdown if clicked outside
            $(document).click(function (e) {
                if (!$dropdown.is(e.target) && $dropdown.has(e.target).length === 0) {
                    $dropdownOptionsParent.find('.options').slideUp();
                    $dropdownTrigger.attr('aria-expanded', 'false');
                    $dropdownOptionsParent.attr('aria-hidden', 'true');
                }
            });
        });
    };

    var lastSelectedState = null; // Store the previously selected option

    animate_sections();

    $(window).scroll(function () {
        animate_sections();
    });

    function animate_sections() {
        var sections = gsap__WEBPACK_IMPORTED_MODULE_0__.gsap.utils.toArray('section');
        sections.forEach(function (sec) {
            gsap__WEBPACK_IMPORTED_MODULE_0__.gsap.from(sec, {
                scrollTrigger: {
                    start: 'top 70%',
                    trigger: sec,
                    onEnter: function onEnter() {
                        sec.classList.add('animate');
                    },
                },
            });
        });
    }

    if ($('.module-list').length > 0) {
        var urlParams = new URLSearchParams(window.location.search);

        // Check if "type" or "filter-year" exists in the query string
        if (urlParams.has('type') || urlParams.has('filter-year') || urlParams.has('pagination')) {
            // Scroll to the .module-list element
            $('html, body').animate(
                {
                    scrollTop: $('.module-list').offset().top - 50,
                },
                1000
            ); // Adjust duration as needed (1000ms = 1 second)
        }
    }

    if ($('.module-hero').length > 0) {
        var hero = $('.module-hero');
        var cards = $('.module-hero').next('.module-spacer').next('.module-cards');

        if (cards.length > 0) {
            $(window).on('scroll', function () {
                if ($(window).scrollTop() > 100) {
                    hero.addClass('hide-heading');
                } else {
                    hero.removeClass('hide-heading');
                }
            });

            gsap__WEBPACK_IMPORTED_MODULE_0__.gsap.fromTo(
                cards,
                {
                    y: 0,
                },
                {
                    y: -300,
                    scrollTrigger: {
                        trigger: cards[0],
                        start: 'top 70%',
                        scrub: true,
                    },
                }
            );
        }
    }

    // Create your instance
    // const gradient = new Gradient();

    // // Call `initGradient` with the selector to your canvas
    // gradient.initGradient('#gradient-canvas');

